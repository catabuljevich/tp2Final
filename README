Desarrollador:
-id
-nombre
-equipo
-tickets (lista)
-proyecto

Manager:
-id
-nombre
-proyectos(lista)

Proyecto:
-id
-nombre
-equipo
-tickets (lista)
-progreso
-completado (bool)
-manager

Equipo:
-id
-nombre
-desarrolladores (lista)
-proyecto

Ticket:
-id
-nombre
-descripcion
-completado (bool)
-dificultad (del 1 al 10)
-prioridad (del 1 al 10)
-proyecto
-desarrollador

